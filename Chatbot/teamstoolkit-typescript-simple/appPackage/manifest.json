{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.2",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Rafferty Uy",
        "websiteUrl": "https://www.raffertyuy.com",
        "privacyUrl": "https://www.raffertyuy.com/raztype/privacy-policy",
        "termsOfUseUrl": "https://www.raffertyuy.com/raztype/privacy-policy"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "RazChatGPT",
        "full": "RazType ChatGPT"
    },
    "description": {
        "short": "RazChatGPT built for Teams",
        "full": "RazType ChatGPT built for Teams"
    },
    "accentColor": "#3860b2",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false,
            "commandLists": [
                {
                    "scopes": [
                        "personal",
                        "team",
                        "groupchat"
                    ],
                    "commands": [
                        {
                            "title": "restart",
                            "description": "Clear and restart this conversation."
                        },
                        {
                            "title": "feedback",
                            "description": "Do you like this experience? Please send your feedback!"
                        }
                    ]
                }
            ]
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}